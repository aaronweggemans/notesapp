<div class="section2" style="margin-top: 150px; margin-bottom: 200px;">
    <hr/>
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <h2 class="slideIn">All notes</h2>
            </div>
        </div>
        <table class="table table-striped">
            <thead>
            <tr>
                <th>Title</th>
                <th>Body</th>
                <th></th>
            </tr>
            </thead>
            <tbody>
            {{#noop}}{{jsondata}}{{/noop}}
            {{#each jsondata}}
                <tr>
                    <td><a href="/edit/notes/{{ title }}">{{ title }}</a></td>
                    <td>{{ body }}</td>
                    <td><a href="/delete/notes/{{ title }}" class="btn btn-danger">Remove</a></td>
                </tr>
            {{/each}}
            </tbody>
        </table>
        <div class="row">
            <div class="col-md-12">
                <h2 class="slideIn">All database users</h2>
                <small class="slideIn">Interactie met database werkt wel, maar word niet per direct uitgevoerd?</small>
            </div>
        </div>
        <table class="table table-striped">
            <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
            </tr>
            </thead>
            <tbody>
            {{#noop}}{{users}}{{/noop}}
            {{#each users}}
                <tr>
                    <td><a href="/edit/user/{{ username }}">{{ username }}</a></td>
                    <td>{{ email }}</td>
                    <td><a href="/delete/user/{{ username }}" class="btn btn-danger">Delete User</a></td>
                </tr>
            {{/each}}
            </tbody>
        </table>
    </div>
    <hr/>
</div>